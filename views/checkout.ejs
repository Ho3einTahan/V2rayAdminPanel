<section class="section-gap">
    <div class="container">
        <div class="row">
            <div class="col-md-4 order-md-2 mb-4">
                <h5 class="d-flex justify-content-between align-items-center mb-4">
                    <span class="text-muted">سبد خرید شما</span>
                    <span class="badge badge-dark badge-pill px-3">3</span>
                </h5>
                <ul class="list-group mb-3">
                    <li class="list-group-item d-flex justify-content-between lh-condensed">
                        <div>
                            <h6 class="my-0">نام محصول</h6>
                            <small class="text-muted">توضیح مختصر</small>
                        </div>
                        <span class="text-muted"> 12 تومان</span>
                    </li>
                    <li class="list-group-item d-flex justify-content-between lh-condensed">
                        <div>
                            <h6 class="my-0">محصول دوم</h6>
                            <small class="text-muted">توضیح مختصر</small>
                        </div>
                        <span class="text-muted"> 8 تومان</span>
                    </li>
                    <li class="list-group-item d-flex justify-content-between lh-condensed">
                        <div>
                            <h6 class="my-0">مورد سوم</h6>
                            <small class="text-muted">توضیح مختصر</small>
                        </div>
                        <span class="text-muted"> 5 تومان</span>
                    </li>
                    <li class="list-group-item d-flex justify-content-between bg-light">
                        <div class="text-success">
                            <h6 class="my-0">کد تبلیغی</h6>
                            <small>کد نمونه</small>
                        </div>
                        <span class="text-success">- 5 تومان</span>
                    </li>
                    <li class="list-group-item d-flex justify-content-between">
                        <span>مجموع (ریال)</span>
                        <strong> 0 تومان</strong>
                    </li>
                </ul>

                <form class="card p-2">
                    <div class="input-group">
                        <input type="text" class="form-control" placeholder="کد تبلیغی">
                        <div class="input-group-append">
                            <button type="submit" class="btn btn-theme">اعمال</button>
                        </div>
                    </div>
                </form>
            </div>
            <div class="col-md-8 order-md-1">
                <h5 class="mb-4">آدرس قبض</h5>
                <form class="needs-validation" novalidate="">
                    <div class="row">
                        <div class="col-md-6 mb-3">
                            <label for="firstName">نام کوچک</label>
                            <input type="text" class="form-control" id="firstName" placeholder="" value="" required="">
                            <div class="invalid-feedback">
                                نام معتبر مورد نیاز است
                            </div>
                        </div>
                        <div class="col-md-6 mb-3">
                            <label for="lastName">نام خانوادگی</label>
                            <input type="text" class="form-control" id="lastName" placeholder="" value="" required="">
                            <div class="invalid-feedback">
                                نام خانوادگی معتبر مورد نیاز است
                            </div>
                        </div>
                    </div>

                    <div class="mb-3">
                        <label for="username">نام کاربری</label>
                        <div class="input-group">
                            <input type="text" class="form-control" id="username" placeholder="نام کاربری" required="">
                            <div class="invalid-feedback" style="width: 100%;">
                                نام کاربری شما مورد نیاز است
                            </div>
                        </div>
                    </div>

                    <div class="mb-3">
                        <label for="email">ایمیل <span class="text-muted">(اختیاری)</span></label>
                        <input type="email" class="form-control" id="email" placeholder="you@example.com">
                        <div class="invalid-feedback">
                            لطفا یک آدرس ایمیل معتبر برای ارسال به روز رسانی وارد کنید
                        </div>
                    </div>

                    <div class="mb-3">
                        <label for="address">آدرس</label>
                        <input type="text" class="form-control" id="address" placeholder="خیابان آذر پلاک 11" required="">
                        <div class="invalid-feedback">
                            لطفا آدرس حمل و نقل خود را وارد کنید.
                        </div>
                    </div>

                    <div class="mb-3">
                        <label for="address2">آدرس 2 <span class="text-muted">(اختیاری)</span></label>
                        <input type="text" class="form-control" id="address2" placeholder="آپارتمان یا مجموعه">
                    </div>

                    <div class="row">
                        <div class="col-md-5 mb-3">
                            <label for="country">کشور</label>
                            <select class="custom-select d-block w-100" id="country" required="">
                                <option value="">انتخاب...</option>
                                <option>ایران</option>
                            </select>
                            <div class="invalid-feedback">
                                لطفا یک کشور معتبر را انتخاب کنید
                            </div>
                        </div>
                        <div class="col-md-4 mb-3">
                            <label for="state">شهر</label>
                            <select class="custom-select d-block w-100" id="state" required="">
                                <option value="">انتخاب...</option>
                                <option>ارومیه</option>
                            </select>
                            <div class="invalid-feedback">
                                لطفا یک کشور معتبر ارائه کنید
                            </div>
                        </div>
                        <div class="col-md-3 mb-3">
                            <label for="zip">کد پستی</label>
                            <input type="text" class="form-control" id="zip" placeholder="" required="">
                            <div class="invalid-feedback">
                                کد پستی مورد نیاز است
                            </div>
                        </div>
                    </div>
                    <hr class="mb-4">
                    <div class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input" id="same-address">
                        <label class="custom-control-label" for="same-address">آدرس حمل و نقل همان آدرس صورت حساب من است</label>
                    </div>
                    <div class="custom-control custom-checkbox">
                        <input type="checkbox" class="custom-control-input" id="save-info">
                        <label class="custom-control-label" for="save-info">این اطلاعات را برای دفعه بعد ذخیره کنید</label>
                    </div>
                    <hr class="mb-4">

                    <h4 class="mb-3">پرداخت</h4>

                    <div class="d-block my-3">
                        <div class="custom-control custom-radio">
                            <input id="credit" name="paymentMethod" type="radio" class="custom-control-input" checked="" required="">
                            <label class="custom-control-label" for="credit">کارت اعتباری</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input id="debit" name="paymentMethod" type="radio" class="custom-control-input" required="">
                            <label class="custom-control-label" for="debit">کارت اعتباری</label>
                        </div>
                        <div class="custom-control custom-radio">
                            <input id="paypal" name="paymentMethod" type="radio" class="custom-control-input" required="">
                            <label class="custom-control-label" for="paypal">پی پال</label>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6 mb-3">
                            <label for="cc-name">نام بر روی کارت</label>
                            <input type="text" class="form-control" id="cc-name" placeholder="" required="">
                            <small class="text-muted">نام کامل بر روی کارت نمایش داده می شود</small>
                            <div class="invalid-feedback">
                                نام در کارت مورد نیاز است
                            </div>
                        </div>
                        <div class="col-md-6 mb-3">
                            <label for="cc-number">شماره کارت اعتباری</label>
                            <input type="text" class="form-control" id="cc-number" placeholder="" required="">
                            <div class="invalid-feedback">
                                شماره کارت اعتباری مورد نیاز است
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-3 mb-3">
                            <label for="cc-expiration">انقضاء</label>
                            <input type="text" class="form-control" id="cc-expiration" placeholder="" required="">
                            <div class="invalid-feedback">
                                تاریخ انقضای لازم است
                            </div>
                        </div>
                        <div class="col-md-3 mb-3">
                            <label for="cc-cvv">CVV</label>
                            <input type="text" class="form-control" id="cc-cvv" placeholder="" required="">
                            <div class="invalid-feedback">
                                کد امنیتی مورد نیاز است
                            </div>
                        </div>
                    </div>
                    <hr class="mb-4">
                    <button class="btn btn-primary btn-lg btn-block" type="submit">ادامه پرداخت</button>
                </form>
            </div>
        </div>
    </div>
</section>
